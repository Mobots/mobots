<package>
  <description brief="Map Visualization">
    Displays map tiles with the image_map_display plugin for rviz
  </description>
  <author>Moritz Ulmer</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/visualization</url>
  <depend package="std_msgs"/>
  <depend package="mobots_msgs"/>
  <depend package="opencv2"/>
  <depend package="cv_bridge"/>
  <depend package="image_transport"/>
  <depend package="rviz"/>
    <depend package="mobots_common"/>
  <export>
    <rosdoc config="${prefix}/rosdoc.yaml"/>
    <rviz plugin="${prefix}/plugin_description.xml"/>
    <cpp cflags="-I${prefix}/include"/>
  </export>
  <platform os="ubuntu" version="9.04"/>
  <platform os="ubuntu" version="9.10"/>
  <platform os="ubuntu" version="10.04"/>
</package>
